<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<table>
  <thead>
    <tr>
      <th>Api venue</th>
      <th>Name</th>
      <th>Timezone</th>
      <th>Address</th>
      <th>City</th>
      <th>State prov</th>
      <th>Postal zip code</th>
      <th>Country</th>
      <th>Tickets url</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Extended address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.api_venue_id %></td>
        <td><%= venue.name %></td>
        <td><%= venue.timezone %></td>
        <td><%= venue.address %></td>
        <td><%= venue.city %></td>
        <td><%= venue.state_prov %></td>
        <td><%= venue.postal_zip_code %></td>
        <td><%= venue.country %></td>
        <td><%= venue.tickets_url %></td>
        <td><%= venue.latitude %></td>
        <td><%= venue.longitude %></td>
        <td><%= venue.extended_address %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
